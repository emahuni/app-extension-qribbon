.q-markdown--table tr:nth-child(odd) {
  background-color: #f2f2f2;
}
.q-markdown code {
  background-color: #bfe0fb!important;
}
.q-markdown pre {
  background-image: linear-gradient(transparent 3%, #bfe0fb 0)!important;
  color: #555;
  background-color: #bfe0fb!important;
}
.q-markdown--code__inner {
  border-left: 2px solid #afa0ff!important;
}
.token.operator, .token.entity, .token.url, .token.variable {
  color: #a67f59;
  background: transparent!important;
}

.ribbon-wrapper.move-50
  .qribbon__vertical--top-left,
  .qribbon__vertical--bottom-left
    left 50px

.ribbon-wrapper.move-100
  .qribbon__vertical--top-left,
  .qribbon__vertical--bottom-left
    left 100px

.ribbon-wrapper
  height 50px

.border-radius
  [class^=qribbon__]
    border-radius 3px 5px 5px 0
